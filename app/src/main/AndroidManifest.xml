        <activity android:name="com.example.abigail.ClubConnect.Club_Instruction"> </activity>
        <activity android:name="com.example.abigail.ClubConnect.NewClubActivity"> </activity>
        <activity android:name="com.example.abigail.ClubConnect.Student_Connect"> </activity>
        <activity android:name="com.example.abigail.ClubConnect.Student_Search"> </activity>
        <activity android:name="com.example.abigail.ClubConnect.Edit_My_Club"> </activity>
        <activity android:name="com.example.abigail.ClubConnect.Instructions_Menu"> </activity>

    </application>

</manifest>